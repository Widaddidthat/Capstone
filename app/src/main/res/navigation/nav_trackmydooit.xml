<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_trackmydooit"
    app:startDestination="@id/splashScreen">

    <activity
        android:id="@+id/splashScreen"
        android:name="com.example.trackmydooit.SplashScreenActivity"
        android:label="activity_splash_screen"
        tools:layout="@layout/activity_splash_screen" />
    <activity
        android:id="@+id/loginActivity"
        android:name="com.example.trackmydooit.LoginActivity"
        android:label="activity_login"
        tools:layout="@layout/activity_login" />
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.trackmydooit.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/expenseActivity"
        android:name="com.example.trackmydooit.ExpenseActivity"
        android:label="activity_expense"
        tools:layout="@layout/activity_expense" />
    <fragment
        android:id="@+id/budgetFragment"
        android:name="com.example.trackmydooit.BudgetFragment"
        android:label="fragment_budget"
        tools:layout="@layout/fragment_budget" />
    <activity
        android:id="@+id/DestGoals"
        android:name="com.example.trackmydooit.CreateSavingsGoalActivity"
        android:label="activity_goals"
        tools:layout="@layout/activity_create_savings_goal"/>
    <activity
        android:id="@+id/wallet_Activity"
        android:name="com.example.trackmydooit.Wallet_Activity"
        android:label="wallet_activity"
        tools:layout="@layout/wallet_activity" />
</navigation>